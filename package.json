{
	"name": "obsidian-runbook",
	"version": "1.0.0",
	"versionComment": "Version synced automatically from manifest.json during build",
	"description": "Execute code blocks directly from your notes using a persistent terminal session.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "node esbuild.config.mjs production",
		"install-plugin": "./scripts/install.sh",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"terminal",
		"runbook",
		"devops"
	],
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"esbuild": "^0.27.2",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"jsdom": "^27.4.0",
		"obsidian": "latest",
		"typescript": "^5.3.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@xterm/addon-fit": "^0.11.0",
		"@xterm/addon-web-links": "^0.12.0",
		"@xterm/xterm": "^6.0.0"
	}
}
